{
  "name": "tesseract",
  "description": "server development box",
  "version": "0.1.15",
  "homepage": "",
  "main": "index.js",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Ahmed Nzibo",
    "email": "a.nzibo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kigenofkenya/tesseract"
  },
  "license": "MIT",
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src-server-feathers",
    "test": "test/"
  },
  "engines": {
    "node": "^9.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/env-cmd ./config/production-cli.env node ./index.js",
    "dev": "./node_modules/.bin/env-cmd ./config/development-cli.env node ./index.js",
    "stage": "./node_modules/.bin/env-cmd ./config/staging-cli.env node ./index.js",
    "build:vue-hot-client": "cross-env NODE_ENV=production webpack --progress --hide-modules  --config src-client-hot-vue/webpack.config.js",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint src-server-feathers/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit",
    "clean": "del public/* '!public/favicon.ico'",
    "clean:react-client": "del build-client-react/* '!build-client-react/index.html'",
    "build:react-client": "webpack --progress --config src-client-react/webpack.config.js",
    "dev:react-client": "webpack-dashboard -- webpack-dev-server -d --hot --config src-client-react/webpack.config.js --watch",
    "production:react-client": "NODE_ENV=production webpack-dashboard -- webpack"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.0",
    "@feathersjs/authentication-client": "^1.0.2",
    "@feathersjs/authentication-jwt": "^1.0.1",
