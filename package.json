    "@feathersjs/feathers": "^3.0.5",
    "@feathersjs/rest-client": "^1.3.3",
    "@feathersjs/socketio": "^3.0.2",
    "@sindresorhus/df": "^2.1.0",
    "async": "^2.6.0",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "cson": "^5.1.0",
    "debug": "^3.1.0",
    "element-ui": "^2.1.0",
    "env-cmd": "^7.0.0",
    "feathers-nedb": "^3.0.0",
    "helmet": "^3.9.0",
    "localstorage-memory": "^1.0.2",
    "markdown-it": "^8.4.0",
    "morgan": "^1.9.0",
    "nedb": "^1.8.0",
